        <script type="text/javascript" src="{{ asset('build/mounthood/js/vendor/revslider/revolution.extension.navigation.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('build/mounthood/js/vendor/revslider/revolution.extension.parallax.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('build/mounthood/js/revslider-index-page.js') }}"></script>

        <script type="text/javascript" src="{{ asset('build/mounthood/js/vendor/woocommerce/js/woocommerce.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('build/mounthood/js/superfish.js') }}"></script>
        <script type="text/javascript" src="{{ asset('build/mounthood/js/core.utils.js') }}"></script>
        <script type="text/javascript" src="{{ asset('build/mounthood/js/core.init.js') }}"></script>
        <script type="text/javascript" src="{{ asset('build/mounthood/js/tpl.init.js') }}"></script>
        <script type="text/javascript" src="{{ asset('build/mounthood/js/shortcodes.js') }}"></script>
        <script type="text/javascript" src="{{ asset('build/mounthood/js/vendor/photostack/modernizr.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('build/mounthood/js/vendor/magnific/jquery.magnific-popup.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('build/mounthood/js/core.messages/core.messages.js') }}"></script>

        <script type="text/javascript" src="{{ asset('build/mounthood/js/vendor/skrollr/dist/skrollr.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('build/mounthood/js/vendor/isotope/dist/isotope.pkgd.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('build/mounthood/js/core.googlemap.js') }}"></script>

        {{ encore_entry_script_tags('app') }}

        <script type="text/javascript" src="{{ asset('build/mounthood/js/revslider-homepage-1.js') }}"></script>

        <script type="text/javascript" src="{{ asset('build/mounthood/js/jquery/ui/widget.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('build/mounthood/js/jquery/ui/tabs.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('build/mounthood/js/jquery/ui/mouse.min.js') }}"></script>
        <script type="text/javascript" src="{{ asset('build/mounthood/js/core.reviews.js') }}"></script>
        <script type="text/javascript" src="{{ asset('build/mounthood/js/jquery/ui/draggable.min.js') }}"></script>

    {% endblock %}

    </body>
</html>
